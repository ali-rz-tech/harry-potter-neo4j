        "\n",
        "for index, row in unique_places.iterrows():\n",
        "    query = \"\"\"\n",
        "    MERGE (p:Place {place_id: $place_id})\n",
        "    ON CREATE SET p.name = $name, p.category = $category\n",
        "    ON MATCH SET p.name = $name, p.category = $category\n",
        "    RETURN p\n",
        "    \"\"\"\n",
        "    parameters = {\n",
        "        \"place_id\": row['Place ID'],\n",
