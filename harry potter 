        "unique_chapters = df_merged[['Chapter ID', 'Chapter Name', 'Movie Chapter']].drop_duplicates()\n",
        "\n",
        "for index, row in unique_chapters.iterrows():\n",
        "    query = \"\"\"\n",
        "    MERGE (c:Chapter {chapter_id: $chapter_id})\n",
        "    ON CREATE SET c.name = $name, c.chapter_number = $chapter_number\n",
        "    ON MATCH SET c.name = $name, c.chapter_number = $chapter_number\n",
        "    RETURN c\n",
        "    \"\"\"\n",
        "    parameters = {\n",
        "        \"chapter_id\": row['Chapter ID'],\n",
        "        \"name\": row['Chapter Name'],\n",
        "        \"chapter_number\": row['Movie Chapter']\n",
        "    }\n",
