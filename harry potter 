      "source": [
        "for index, row in df_merged.iterrows():\n",
        "    # Create or update Dialogue node\n",
        "    run_query(\n",
        "        \"MERGE (d:Dialogue {dialogue_id: $dialogue_id}) ON CREATE SET d.text = $text ON MATCH SET d.text = $text RETURN d\",\n",
        "        {\n",
        "            \"dialogue_id\": row['Dialogue ID'],\n",
        "            \"text\": row['Dialogue']\n",
        "        }\n",
        "    )\n",
        "\n",
        "    # Establish BELONGS_TO_CHAPTER relationship\n",
        "    run_query(\n",
        "        \"MATCH (d:Dialogue {dialogue_id: $dialogue_id}), (c:Chapter {chapter_id: $chapter_id}) MERGE (d)-[:BELONGS_TO_CHAPTER]->(c)\",\n",
        "        {\n",
        "            \"dialogue_id\": row['Dialogue ID'],\n",
        "            \"chapter_id\": row['Chapter ID']\n",
        "        }\n",
        "    )\n",
        "\n",
        "    # Establish PART_OF_MOVIE relationship\n",
        "    run_query(\n",
        "        \"MATCH (c:Chapter {chapter_id: $chapter_id}), (m:Movie {movie_id: $movie_id}) MERGE (c)-[:PART_OF_MOVIE]->(m)\",\n",
        "        {\n",
        "            \"chapter_id\": row['Chapter ID'],\n",
        "            \"movie_id\": row['Movie ID']\n",
        "        }\n",
        "    )\n",
        "\n",
        "    # Establish OCCURRED_AT relationship\n",
        "    run_query(\n",
        "        \"MATCH (d:Dialogue {dialogue_id: $dialogue_id}), (p:Place {place_id: $place_id}) MERGE (d)-[:OCCURRED_AT]->(p)\",\n",
        "        {\n",
        "            \"dialogue_id\": row['Dialogue ID'],\n",
        "            \"place_id\": row['Place ID']\n",
        "        }\n",
        "    )\n",
        "\n",
        "    # Establish SPOKEN_BY relationship\n",
        "    run_query(\n",
        "        \"MATCH (d:Dialogue {dialogue_id: $dialogue_id}), (ch:Character {character_id: $character_id}) MERGE (d)-[:SPOKEN_BY]->(ch)\",\n",
        "        {\n",
        "            \"dialogue_id\": row['Dialogue ID'],\n",
        "            \"character_id\": row['Character ID']\n",
        "        }\n",
        "    )\n",
        "\n",
        "print(\"Dialogue nodes and relationships created/updated in AuraDB.\")\n",
        "print(\"Please ensure your AuraDB connection details are correct and active to successfully upload the data.\")"
      ],
