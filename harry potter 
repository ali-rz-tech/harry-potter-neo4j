        "id": "57423bc4",
        "outputId": "da5b4ba9-25fe-4d1c-f973-9827d328faa1"
      },
      "source": [
        "unique_places = df_merged[['Place ID', 'Place Name', 'Place Category']].drop_duplicates()\n",
        "\n",
        "for index, row in unique_places.iterrows():\n",
        "    query = \"\"\"\n",
        "    MERGE (p:Place {place_id: $place_id})\n",
        "    ON CREATE SET p.name = $name, p.category = $category\n",
        "    ON MATCH SET p.name = $name, p.category = $category\n",
        "    RETURN p\n",
        "    \"\"\"\n",
        "    parameters = {\n",
        "        \"place_id\": row['Place ID'],\n",
        "        \"name\": row['Place Name'],\n",
        "        \"category\": row['Place Category']\n",
        "    }\n",
        "    run_query(query, parameters)\n",
        "\n",
        "print(\"Unique Place nodes created/updated in AuraDB.\")"
      ],
