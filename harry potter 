      "source": [
        "df_merged['text_for_embedding'] = df_merged['Dialogue'].fillna('') + ' ' + \\\n",
        "                                   df_merged['Character Name'].fillna('') + ' ' + \\\n",
        "                                   df_merged['Movie Title'].fillna('') + ' ' + \\\n",
        "                                   df_merged['Chapter Name'].fillna('') + ' ' + \\\n",
        "                                   df_merged['Place Name'].fillna('')\n",
        "\n",
